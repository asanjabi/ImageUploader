    //{1} = File name
    //{2} = Day
    //{3} = Month
    //{4} = Year
    //{5} = DayOfWeek
    //{6} = DayofYear
    //{7} = Hour
    //{8] = Hour24
    //{9} = Minute
    //{10} = Second
    //{11} = Ticks
    "FileNameFormatString": "{0}\\{4}-{3}-{2}--{8}-{9}-{10}--{1}",
    "MaxFileSizeInMB": 10
  }
